<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.api.mapper.ArticleMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.api.pojo.Article">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="articleName" column="article_name" jdbcType="VARCHAR"/>
            <result property="articleType" column="article_type" jdbcType="INTEGER"/>
            <result property="articlePath" column="article_path" jdbcType="VARCHAR"/>
            <result property="linkPhone" column="link_phone" jdbcType="VARCHAR"/>
            <result property="articleFrom" column="article_from" jdbcType="INTEGER"/>
            <result property="articleText" column="article_text" jdbcType="VARCHAR"/>
            <result property="articleDay" column="article_day" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,article_name,article_type,
        article_path,link_phone,article_from,
        article_text,article_day
    </sql>
</mapper>
